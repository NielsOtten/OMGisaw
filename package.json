{
  "name": "omgisaw",
  "scripts": {
    "start": "nodemon src/server/index.ts",
    "build": "NODE_ENV=production npm run build:server && npm run build:client",
    "build:client": "rimraf src/.next && next build ./src",
    "build:server": "rimraf build/ && tsc --project tsconfig.server.json",
    "serve": "NODE_ENV=production node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nylsoo/OMGisaw.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nylsoo/OMGisaw/issues"
  },
  "homepage": "https://github.com/nylsoo/OMGisaw#readme",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "compression": "^1.7.4",
    "connect-slashes": "^1.4.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "linaria": "^1.3.1",
    "next": "^9.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/connect-slashes": "0.0.31",
    "@types/express": "^4.17.0",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/next": "^8.0.6",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  },
  "version": "0.0.0-dev",
  "nodemonConfig": {
    "exec": "babel-node --extensions \".ts\"",
    "watch": [
      "src/server/*"
    ]
  }
}
